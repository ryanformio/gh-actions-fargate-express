{
    "family": "github-runner",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "runner-container",
        "image": "YOUR_DOCKER_IMAGE_URL_HERE",
        "cpu": 256,
        "memory": 512,
        "memoryReservation": 256,
        "essential": true,
        "environment": [
          {
            "name": "GITHUB_REPO_URL",
            "value": "YOUR_REPO_URL"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "YOUR_LOG_GROUP_NAME",
            "awslogs-region": "YOUR_AWS_REGION",
            "awslogs-stream-prefix": "github-runner"
          }
        }
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }
  